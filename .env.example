# Copy this file to .env.local (Next.js) and fill in real credentials.
# NEVER commit your real credentials.

EMAIL_USER=youremail@gmail.com
# For Gmail use an App Password (16 chars) not your normal password.
EMAIL_PASS=your_app_password_here

# Optional override recipients (defaults fall back to EMAIL_USER)
BOOKING_NOTIFY_TO=owner@example.com
COUPLES_NOTIFY_TO=owner@example.com
RETREAT_NOTIFY_TO=owner@example.com
